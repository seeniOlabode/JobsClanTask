{"ast":null,"code":"import InputComponentVue from \"@/components/shared/InputComponent.vue\";\nimport { multiForm } from \"@/App.vue\";\nimport { ValidateEmail, ValidateName, ValidatePhoneNumber } from \"@/helpers/validation\";\nexport default {\n  name: \"PersonalInfo\",\n  components: {\n    InputComponentVue\n  },\n  data() {\n    return {\n      store: multiForm,\n      validation: {\n        email: ValidateEmail,\n        name: ValidateName,\n        phone: ValidatePhoneNumber\n      }\n    };\n  }\n};","map":{"version":3,"mappings":"AA0CA,OAAOA,iBAAgB,MAAO,wCAAwC;AAEtE,SAASC,SAAQ,QAAS,WAAW;AAErC,SACEC,aAAa,EACbC,YAAY,EACZC,mBAAmB,QACd,sBAAsB;AAE7B,eAAe;EACbC,IAAI,EAAE,cAAc;EACpBC,UAAU,EAAE;IAAEN;EAAkB,CAAC;EACjCO,IAAI,GAAG;IACL,OAAO;MACLC,KAAK,EAAEP,SAAS;MAChBQ,UAAU,EAAE;QACVC,KAAK,EAAER,aAAa;QACpBG,IAAI,EAAEF,YAAY;QAClBQ,KAAK,EAAEP;MACT;IACF,CAAC;EACH;AACF,CAAC","names":["InputComponentVue","multiForm","ValidateEmail","ValidateName","ValidatePhoneNumber","name","components","data","store","validation","email","phone"],"sourceRoot":"","sources":["/Users/macbookpro/Desktop/my-projects/multi-step-form/src/components/form/formTabs/PersonalInfo.vue"],"sourcesContent":["<template>\n  <div class=\"form-modal\">\n    <h2 class=\"font-bold text-xl text-multi-denim\">Personal Info</h2>\n    <p\n      class=\"text-multi-gray font-normal mt-2 text-base\"\n      style=\"line-height: 25px\"\n    >\n      Please provide your name, email address, and phone number.\n    </p>\n\n    <form class=\"mt-6\">\n      <input-component-vue\n        title=\"Name\"\n        type=\"text\"\n        placeholder=\"e.g. Stephen King\"\n        v-model=\"store.tabs.get('1').data.name\"\n        ref=\"name\"\n        :validationCallback=\"validation.name\"\n      />\n      <input-component-vue\n        class=\"mt-6\"\n        title=\"Email\"\n        type=\"email\"\n        placeholder=\"e.g. stephenking@lorem.com\"\n        v-model=\"store.tabs.get('1').data.email\"\n        ref=\"email\"\n        :validationCallback=\"validation.email\"\n      />\n      <input-component-vue\n        class=\"mt-6\"\n        title=\"Phone Number\"\n        type=\"tel\"\n        placeholder=\"e.g. +1 234 567 890\"\n        v-model=\"store.tabs.get('1').data.phone\"\n        ref=\"phone\"\n        :validationCallback=\"validation.phone\"\n      />\n    </form>\n  </div>\n</template>\n\n<script>\nimport InputComponentVue from \"@/components/shared/InputComponent.vue\";\n\nimport { multiForm } from \"@/App.vue\";\n\nimport {\n  ValidateEmail,\n  ValidateName,\n  ValidatePhoneNumber,\n} from \"@/helpers/validation\";\n\nexport default {\n  name: \"PersonalInfo\",\n  components: { InputComponentVue },\n  data() {\n    return {\n      store: multiForm,\n      validation: {\n        email: ValidateEmail,\n        name: ValidateName,\n        phone: ValidatePhoneNumber,\n      },\n    };\n  },\n};\n</script>\n\n<style scoped>\n:global(.form-modal) {\n  @apply bg-white mx-4 rounded-md py-8 px-6 shadow;\n}\n</style>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}