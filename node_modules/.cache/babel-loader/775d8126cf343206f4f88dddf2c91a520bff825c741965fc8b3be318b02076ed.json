{"ast":null,"code":"import RadioComponent from \"@/components/shared/RadioComponent.vue\";\nimport ToggleComponent from \"@/components/shared/ToggleComponent.vue\";\nimport { Plan } from \"@/models\";\nexport default {\n  name: \"SelectPlan\",\n  components: {\n    RadioComponent: RadioComponent,\n    ToggleComponent: ToggleComponent\n  },\n  data: function data() {\n    return {\n      plan: \"\",\n      formData: new Plan()\n    };\n  },\n  methods: {\n    setPlan: function setPlan(input) {\n      this.formData.plan = input;\n      this.plan = input;\n    }\n  },\n  computed: {\n    formComplete: function formComplete() {\n      return this.formData.plan != \"\";\n    }\n  }\n};","map":{"version":3,"mappings":"AAsBA,OAAOA,cAAa,MAAO,wCAAwC;AACnE,OAAOC,eAAc,MAAO,yCAAyC;AAErE,SAASC,IAAG,QAAS,UAAU;AAE/B,eAAe;EACbC,IAAI,EAAE,YAAY;EAClBC,UAAU,EAAE;IACVJ,cAAc,EAAdA,cAAc;IACdC,eAAe,EAAfA;EACF,CAAC;EACDI,IAAI,kBAAG;IACL,OAAO;MACLC,IAAI,EAAE,EAAE;MACRC,QAAQ,EAAE,IAAIL,IAAI;IACpB,CAAC;EACH,CAAC;EACDM,OAAO,EAAE;IACPC,OAAO,mBAACC,KAAK,EAAE;MACb,IAAI,CAACH,QAAQ,CAACD,IAAG,GAAII,KAAK;MAC1B,IAAI,CAACJ,IAAG,GAAII,KAAK;IACnB;EACF,CAAC;EACDC,QAAQ,EAAE;IACRC,YAAY,0BAAG;MACb,OAAO,IAAI,CAACL,QAAQ,CAACD,IAAG,IAAK,EAAE;IACjC;EACF;AACF,CAAC","names":["RadioComponent","ToggleComponent","Plan","name","components","data","plan","formData","methods","setPlan","input","computed","formComplete"],"sourceRoot":"","sources":["/Users/macbookpro/Desktop/multi-form-project/src/components/form/formTabs/SelectPlan.vue"],"sourcesContent":["<template>\n  <div class=\"form-modal\">\n    <h2 class=\"font-bold text-xl text-multi-denim\">Select your plan</h2>\n    <p\n      class=\"text-multi-gray font-normal mt-2 text-base\"\n      style=\"line-height: 25px\"\n    >\n      You have the option of monthly or yearly billing.\n    </p>\n\n    <form class=\"mt-6\">\n      <radio-component @plan=\"setPlan\" :yearly=\"formData.yearly\" />\n      <toggle-component\n        :yearly=\"formData.yearly\"\n        @toggle-plan=\"() => (formData.yearly = !formData.yearly)\"\n        class=\"mt-6\"\n      />\n    </form>\n  </div>\n</template>\n\n<script>\nimport RadioComponent from \"@/components/shared/RadioComponent.vue\";\nimport ToggleComponent from \"@/components/shared/ToggleComponent.vue\";\n\nimport { Plan } from \"@/models\";\n\nexport default {\n  name: \"SelectPlan\",\n  components: {\n    RadioComponent,\n    ToggleComponent,\n  },\n  data() {\n    return {\n      plan: \"\",\n      formData: new Plan(),\n    };\n  },\n  methods: {\n    setPlan(input) {\n      this.formData.plan = input;\n      this.plan = input;\n    },\n  },\n  computed: {\n    formComplete() {\n      return this.formData.plan != \"\";\n    },\n  },\n};\n</script>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}